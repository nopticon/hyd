body
	padding 20px
	background url(images/wood2.jpg) fixed top center #202837

p, div, span, select
	cursor default

p, div, span, input, textarea, select, a, optgroup
	font-family 'Open Sans', Verdana, Arial, Helvetica, sans-serif
	text-decoration none
	font-weight lighter
	font-size 100%
	color #d0ced1

h1, h2, h3
	font-family 'Raleway', Verdana, Arial, Helvetica, sans-serif
	text-decoration none

select, optgroup
	color #eee

h1, h2
	color #868670

a
	color #c3c2c4
	&:hover
		text-decoration underline

img
	vertical-align middle
	max-width 100%

input, button, textarea, select
	background #41413E
	padding 2px
	border 0

#content
	padding 10px
	clear both
	border 1px solid #111
	background rgba(0,0,0,0.6)

#menu
	display block
	clear both
	float left
	margin 3px 0 0 0
	ul
		list-style none
		margin 0
		padding 0
		border 0
		li
			float left
			margin 0 8px 0 0
			a
				margin 5px
				display inline

#sign
	float right
	margin 8px 0 0 0
	ul
		margin 0
		padding 0
		li
			float left
			list-style none
			a
				padding 7px 10px
				background #F5B800
				color #000
				border 1px solid #DBA400
				&:hover
					text-decoration none
					background #DBA400
			ul
				display none

#search
	width 30%
	float right
	position relative
	padding 0
	left 0
	top 0
	fieldset
		margin-top 3px
		border 0
		text-align right
	input
		background #444
		border 0 solid #383838
		box-shadow 0 !important
		color #d0ced1
		margin 0 3px 0 5px
		padding 1px
		&.submit
			padding 1px
			font-weight normal

#search_result
	display none
	margin 10px 0 15px 0
	color #EEE
	padding 10px
	overflow auto
	border 1px solid #111
	background rgba(0,0,0,0.8)
	input
		padding 4px
		background #5DA209
		color #fff

#footer
	padding 22px 0 25px 0

.new_header
	a
		font-family "Bitter"
		font-size 300%
		color #eee

.page
	width 1170px
	margin 0 auto

.header
	display block
	width 100%
	margin 5px 0 5px 0
	overflow hidden

.brand
	width 70%
	float left
	h1
		width 300px
		height 51px
		a
			display block
			width 300px
			height 47px
			background url(images/header.gif) no-repeat
			text-indent 100%
			white-space nowrap
			overflow hidden
			&:hover
				background-position 0 -47px
	h2
		display none

.soc
	margin 3px 0 0
	float right
	position relative

.defaultInput
	font-size 10px
	color #BBB !important

.flying
	display none
	background rgba(0,0,0,0.8)
	position absolute
	padding 3px
	overflow auto
	border-right 1px solid #161C26
	border-left 1px solid #161C26
	border-bottom 1px solid #161C26
	border-top-left-radius 10px
	border-bottom-right-radius 10px
	li
		float none !important
		margin 15px
		a
			color #7AD40C

#soc_facebook
	color #65A8D0

#soc_twitter
	color #9AE4E8

.h
	margin 5px 0 10px
	h3
		a
			&:hover
				text-decoration none

.h h3, .h h3 a
	display inline
	margin 0 10px
	font-family 'Open Sans', Arial, Helvetica, sans-serif !important
	font-size 14pt
	color #F5B800

.b
	h3
		a
			padding 5px
			background #222
			&:hover
				background #111

.hg
	font-weight bold

.w
	width 50%
	margin 20px auto
	padding 10px
	background rgba(200,200,0,0.2)
	text-align center

.w h3, .w p
	margin 0
	text-align center

.z
	display none !important

.ls_separe
	list-style none
	padding 0
	margin 0
	li
		padding 3px 3px 3px 10px
		margin-bottom 6px
		border-left 2px solid #444
		&:hover
			border-left 2px solid #555

.wm_ls
	margin 0 !important

.wm
	width 45%
	float left
	padding 10px !important
	list-style none
	._
		font-size 11pt

.wms
	list-style none
	margin 0
	padding 0
	li
		padding 10px 10px 5px 0 !important
	._
		list-style none

.profile
	margin 0 0 8px
	h3
		display inline
		margin 0
		border-bottom 1px solid #F5B800
		padding-bottom 3px
		font-family 'Ubuntu', Arial, Helvetica, sans-serif !important
		font-size 13pt
		color #F5B800

.smile
	font-size 20px
	&:hover
		text-decoration none
		color #A4DB00

.lc2
	ul
		li
			float left
			width 24%
			ul
				li
					float none
					width 100%

.ln, .ls_inline

.ln ul, .ls_inline ul
	list-style none
	margin 0
	padding 0
	border 0

.ls_inline
	ul
		li
			float left
			margin-left 25px

.ls
	ul
		margin 0 0 5px 25px
		padding 0
		border 0
		ul
			list-style none
			margin 0
			padding 0
		li
			padding 5px 0
			li
				padding 0

.l2
	ul
		li
			margin 3px 0 0 5px
			width 49%
			float left

.l2 ul li ul li, .l3 ul li ul li, .l4 ul li ul li, .l6 ul li ul li
	margin 0
	width 100%
	float none
	overflow visible
	height auto

.l2 ._, .lc ._, .l3 ._, .l3 ._, .l6 ._
	margin 5px 0 5px 5px

.l2 .__, .lc .__, .l3 .__, .l4 .__, .l6 .__
	margin 5px 0

.lc
	ul
		li
			margin-bottom 10px
			ul
				li
					margin-bottom 0
					text-align center

.l3
	ul
		li
			margin 10px 0 0 5px
			width 32%
			float left

.l4
	ul
		li
			margin 10px 0 0 5px
			width 24%
			float left

.l5
	ul
		li
			margin 10px 0 0 5px
			width 19%
			float left

.l6
	ul
		li
			margin 0 0 0 5px
			width 16%
			height 100px
			float left
			overflow hidden

.lf
	ul
		li
			height 140px

.lf2
	ul
		li
			height 110px

.lf ul li ul li, .lf2 ul li ul li
	height auto

.el_left
	float left

.el_right
	float right

.is_left
	width 73%
	float left

.is_right
	width 25%
	float right

.is_left_i
	width 25%
	float left

.is_right_i
	width 75%
	float right

.is_mid
	width 50%
	float left

.pad_left
	margin-left 10px

.pad_right
	margin-right 10px

.pad_top
	margin-top 10px

.x2
	margin-top 20px

.x3
	margin-top 30px

.in_right
	padding-right 10px

.a_left
	text-align left

.a_right
	text-align right

.a_mid
	text-align center

.i input, .i textarea
	width 100%

._, ._ a, .__, .__ a
	color #9F9FA2 !important

.ctl
	ul
		list-style none
		margin 0
		padding 0
		border 0
		li
			float left
			margin-right 25px
			a
				&:hover
					text-decoration none
					background #518c08
	input
		padding 10px

.ctl_lf
	margin 15px 0 0 43px

.ctl_lw
	margin 25px 0 0 33px

.ctl ul li a, .ctl ul li strong, .ctl ul li span, .ctl input, .ctl ul li small, input.ctl, .publish input, .remove
	margin 10px 0 0
	padding 5px 15px
	border-radius 5px
	background #64AE0A
	color #fff

.ctl ul li span, .ctl ul li strong
	border 1px solid #999

.p
	margin 15px 0 15px 0
	text-align center
	padding 4px
	ul
		list-style none
		margin 0
		padding 0
		border 0
		li
			float left
			margin-right 15px

.p strong, .p a
	line-height 30px
	text-decoration none
	padding 5px
	background #202837

.p strong, .p a:hover
	text-decoration none
	background #5d8812

.publish
	margin 0 auto
	form
		padding 8px
	input
		display block
		margin 10px 0 0
		padding 4px 8px
		background #5DA209
		color #fff
	textarea
		margin 0 auto
		display block
		width 100%
	label
		padding 10px 0 0
		margin 0 10px 0 0
	.f
		display inline
		width 25%
	.ff
		margin 0 0 10px 0

.publish textarea, .publish input.f
	background rgba(150,150,150,0.3)

.tt
	background #fff

.submit
	display block
	margin 10px 0 0
	padding 4px 8px
	background #5DA209
	color #fff

.remove
	background #C23000

.nowrapcell
	white-space nowrap

.nowraptext
	overflow auto
	a
		color #66CC33

.comments
	display block
	clear both
	margin 0 2%
	h3
		position relative
		float left
		width 20%
		margin 1em 0 0 0
		padding 1.5em 0 0 0
		font-size 1em
		border-top 1px solid #111
		text-align center
		span
			position absolute
			left -9999px
			text-align left
		strong
			display none
			position absolute
			left 0
			top -0.6em
			padding 0 0.2em 0 14px
			font 300 1.5em/1em arial,sans-serif
			color #b0b0a6
		em
			display block
			text-align center
			font-size 0.95em
			font-style normal
			color #fff
			img
				padding 1px
		cite
			display block
			font-style normal
			margin 4px 0 5px 0
	blockquote
		position relative
		float right
		width 77%
		margin 1em 0 0 0
		padding 1.5em 0
		border-top 1px solid #222
		p
			&.dtposted
				position absolute
				top 0
				right 0
				margin 0
				padding 8px
				line-height auto
				text-align right
				background #222
				a
					padding-left 14px
			&.controls
				position absolute
				top 0
				left 0
				margin 0
				padding 8px
				line-height auto
				background #64AE0A
				a
					position relative
					overflow hidden
					top 0
					margin 0
					padding 0 15px
					height auto
					border-left 1px solid #569509
					color #fff
					&:first-child
						border 0
		div
			line-height 1.8em
			color #d0ced1
		blockquote
			overflow hidden
			margin 1em 0
			padding 0.2em 1.25em 0.2em 24px
			background rgba(0,0,0,0.6)
			color #d0ced1
			float none
			width auto
			margin 1em 0
			border 0

.thumbnail_view
	width 275px
	height 150px
	border 1px solid #e5e5e5
	overflow hidden
	margin 10px auto
	img
		position relative

.poll
	.bar
		display block
		border 1px solid #161C26
		height 5px
		div
			display block
			height 5px
			line-height 5px
			background #70a416
			border-right 1px solid #161C26

.clear
	clear both
	display block

.clearfix
	zoom 1
	&:after
		clear both

.clearfix:before, .clearfix:after
	content "\0020"
	display block
	height 0
	overflow hidden

html, body
	height 100%

#ps_hover
	left 0
	top 0
	position absolute
	z-index 1000

.ps_hd, .ps_bd, .ps_ft
	position relative
	z-index 1000

.ps_hd .ps_c,.ps_ft .ps_c
	font-size 1px
	height 8px

#ps_title
	background url(images/tt_r.png) top right no-repeat
	padding 0 5px 0
	font-size 10px
	font-weight normal
	left 8px
	position absolute
	top 0
	z-index 999
	.ps_tt_l
		background url(images/tt_l.png) top left no-repeat
		line-height 20px
		padding 0 0 0 5px

#ps_tooltip
	font-size 10px
	left 0
	position absolute
	top 0
	width 250px
	z-index 1010
	p
		margin 0
	strong
		font-size 1.2em

#ps_websites
	left 50%
	position absolute
	top 50%
	z-index 1005
	.ps_label
		color #fff
		display none
		font-size 2em
		left 0
		position absolute
		top 0
		white-space nowrap
	ul
		list-style none
		position relative
		z-index 1010
		width 400px
	li
		display block
		float left
		margin 0 30px 40px 0
	a
		display block

#ps_overlay
	background #000
	height 100%
	left 0
	position absolute
	top 0
	width 100%
	z-index 1005

.cb-enable, .cb-disable, .cb-enable span, .cb-disable span
	background url(images/switch.gif) repeat-x
	display block
	float left

.cb-enable span, .cb-disable span
	line-height 30px
	display block
	background-repeat no-repeat
	font-weight bold

.cb-enable
	span
		background-position left -90px
		padding 0 10px
	&.selected
		background-position 0 -60px
		span
			background-position left -150px
			color #fff

.cb-disable
	span
		background-position right -180px
		padding 0 10px
	&.selected
		background-position 0 -30px
		span
			background-position right -210px
			color #fff

.switch
	label
		cursor pointer
	input
		display none

.company_title
	width 90%
	font-size 21px
	margin 30px auto 30px
	text-align center

.company_sub
	width 75%
	font-size 19px
	color #eee
	margin 0 auto 30px
	text-align center

.social_share
	margin 5px 27% 0 27%
	height 200px

.form_align
	label
		width 250px
		float left
		position relative
	select
		width 75px

ul
	&.holder
		margin 0
		overflow hidden
		height auto !important
		height 1%
		padding 4px 5px 0
		background none repeat scroll 0% 0% rgba(150, 150, 150, 0.3)
		margin 0 !important
		li
			float left
			list-style-type none
			margin 0 5px 4px 0
			white-space nowrap
			&.bit-box
				-moz-border-radius 6px
				-webkit-border-radius 6px
				border-radius 6px
				border 1px solid #CAD8F3
				padding 1px 5px 2px
				a
					&.closebutton
						position absolute
						right 4px
						top 5px
						display block
						width 7px
						height 7px
						font-size 1px
						background url('close.gif')
						&:hover
							background-position 7px
			&.bit-box-focus
				border-color #598BEC
				background #598BEC
				color #fff
				border-color #598BEC
				background #598BEC
				color #fff
			&.bit-input
				input
					width auto
					background none !important
					overflow visible
					margin 0
					border 0px
					outline 0
					padding 3px 0px 2px
					&.smallinput
						width 20px
			&.bit-hover
				background #BBCEF1
				border 1px solid #6D95E0

*:first-child
	& + html
		ul
			&.holder
				padding-bottom 2px

*
	html
		ul
			&.holder
				padding-bottom 2px

ul.holder li.bit-box, ul.holder li.bit-input input
	font 11px "Lucida Grande", "Verdana"

ul.holder li.bit-box, #apple-list ul.holder li.bit-box
	padding-right 15px
	position relative
	z-index 1000

#apple-list
	ul
		&.holder
			li
				&.bit-input
					margin 0
					input
						&.smallinput
							width 5px

ul.holder li.bit-box-focus a.closebutton, ul.holder li.bit-box-focus a.closebutton:hover
	background-position bottom

.facebook-auto
	display none
	background #000
	z-index 1001
	.select_all_items
		display block
		float right
	.default
		padding 5px 7px
		border 1px solid #ccc
		border-width 0 1px 1px
		font-family "Lucida Grande","Verdana"
		font-size 11px
	ul
		display none
		margin 0
		padding 0
		overflow auto
		position absolute
		z-index 9999
		li
			padding 5px 12px
			z-index 1000
			cursor pointer
			margin 0
			list-style-type none
			border 1px solid #ccc
			border-width 0 1px 1px
			font 11px "Lucida Grande", "Verdana"
			background-color #eee
			em
				font-weight bold
				font-style normal
				background #ccc
			&.auto-focus
				background #4173CC
				color #fff
				em
					background none

.deleted
	background-color #4173CC !important
	color #ffffff !important

.hidden
	display none

